{
  "port": 3921,
  "reqbody": {
    "maxlen": 4096
  },
  "gzip": {
    "size": 4096
  },
  "vfs": {
    "batch": {
      "timeout": 50
    },
    "watch": {
      "timeout": 50
    }
  },
  "dirs": {
    "base": "/var/lib/ihbh",
    "kvs": {
      "feedbacks": "kvs/feedbacks",
      "reports": "kvs/reports",
      "places": "kvs/places",
      "map": "kvs/map",
      "user": "kvs/user"
    }
  },
  "rsync": {
    "maxFilePathLen": 1024,
    "maxFileSizeLen": 4096
  },
  "cache": {
    "maxUserDbSize": 10000
  },
  "cert": {
    "dir": "/etc/letsencrypt/live/data.ihbh.org/",
    "keyfile": "privkey.pem",
    "certfile": "cert.pem"
  },
  "map": {
    "cell": 1024
  }
}