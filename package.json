{
  "scripts": {
    "start": "npm run build; sudo mkdir -p /var/log/ihbh/srv; npm run server | tee /var/log/ihbh/srv/1.log",
    "clean": "rm -rf bin",
    "build": "npm run clean; npm i; tsc",
    "server": "sudo node bin/index --verbose",
    "test": "npm run build; node tests/func",
    "ssh": "ssh root@ihbh.org",
    "ssh-clean": "ssh root@ihbh.org 'rm -rf ~/ihbh && mkdir -p ~/ihbh'",
    "ssh-upload": "npm run ssh-clean && scp -r src *.json root@ihbh.org:~/ihbh",
    "ssh-restart": "ssh root@ihbh.org 'fuser -vks 3921/tcp && cd ~/ihbh; npm start'",
    "ssh-upgrade": "npm run ssh-upload && npm run ssh-restart",
    "ssh-netstat": "ssh root@ihbh.org 'netstat -ltp'",
    "ssh-diskstat": "ssh root@ihbh.org 'du -h -d 2 /var/lib/ihbh'"
  },
  "dependencies": {
    "@types/lru-cache": "^5.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.7",
    "supercop.wasm": "5.0.1",
    "commander": "2.19.0",
    "lru-cache": "5.1.1",
    "mkdirp": "0.5.1",
    "sha1": "1.1.1",
    "typescript": "3.6.3"
  }
}
